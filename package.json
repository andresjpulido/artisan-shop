{
  "name": "shop",
  "version": "1.0.0",
  "description": "Web application that manages the inventory, the creation of purchase orders, the registration of hours you work and the creation of the weekly payslip",
  "main": "app.js",
  "scripts": {
    "dev1": "nodemon --exec babel-node ./server/app.js",
    "dev": "cd client && npm run build && cd .. && nodemon --exec babel-node ./server/app.js",
    "clean": "rm -rf build && mkdir build",
    "babel-build": "babel -d ./build ./server -s",
    "build": "npm run clean && npm run babel-build",
    "start": "npm run build && node ./build/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andresjpulido/shop.git"
  },
  "keywords": [
    "heroku",
    "nodejs",
    "reactjs",
    "postgres",
    "rest"
  ],
  "author": "Andres Pulido",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andresjpulido/shop/issues"
  },
  "homepage": "https://github.com/andresjpulido/shop#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.5.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.7.1",
    "handlebars": "^4.7.7",
    "jwt-simple": "^0.5.6",
    "method-override": "^2.1.2",
    "moment": "^2.24.0",
    "pdfmake": "^0.1.60",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "postgres": "^1.0.2",
    "puppeteer": "^1.20.0",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
